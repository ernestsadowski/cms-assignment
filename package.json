    "react-device-detect": "^2.2.3",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "sanity": "^4.1.1",
    "sanity-plugin-dashboard-widget-vercel": "^3.1.4",
    "sanity-plugin-simpler-color-input": "^3.1.1",
    "server-only": "^0.0.1",
    "shiki": "^3.8.1",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.6"
